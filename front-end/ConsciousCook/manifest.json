{
    "name": "Conscious Cook",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches" : ["<all_urls>"],
            "js": ["content.js",
            "tippy.js"],
            "css" : ["content.css"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Conscious Cook",
        "default_description": "A tool to reduce your carbon footprint."
        
    },
    "permissions": [
        "tabs"
    ]
}
